# path: .env.example
# API
VITE_API_BASE=/api/v1

# WS base (optional). If empty -> derived from location (wss://host)
VITE_WS_BASE=

# guest | user (optional). If empty -> auto: guest until authenticated
VITE_APP_DEFAULT_MODE=guest

# Enable verbose logger in dev/prod
VITE_LOG_LEVEL=info

# Telegram (optional)
VITE_TG_FORCE_ENABLE=false
